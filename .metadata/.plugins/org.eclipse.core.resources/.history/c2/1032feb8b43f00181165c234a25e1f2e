package demo2;


 class Runner implements Runnable{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		
		for(int i=0;i<10;i++){
			try {
				System.out.println("Hi"+i);
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
		
	}
	
}
public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
