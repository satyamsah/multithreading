package demo;

import java.util.*;

public class Worker {

	private List<Integer> list1= new ArrayList<>();
	private List<Integer> list2= new ArrayList<>();
	
	
	public void worker1(){
		
		Thread.sleep(1);
		
	}
	
	public void worker2(){
		
		Thread.sleep(1);
		
	}
	
	
	public void process(){
		worker1();
		worker2();
		
	}
	
	
	public void call(){
		
		process();
		
	}
	
}
